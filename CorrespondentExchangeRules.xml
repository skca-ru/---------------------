	|				ИЛИ СписаниеСРасчетногоСчета.ДокументОснование = ПлатежноеПоручение.Ссылка)
	|ГДЕ
	|	СписаниеСРасчетногоСчета.Ссылка = &amp;Ссылка");

Запрос.УстановитьПараметр("Ссылка", Источник.Ссылка);
Выборка = Запрос.Выполнить().Выбрать();
Если Выборка.Следующий() Тогда
	ЗаполнитьЗначенияСвойств(ВходящиеДанные, Выборка);
Иначе
	ВходящиеДанные.ИдентификаторПлатежа = "";
КонецЕсли;</Текст>
				<Параметры>Источник, ВходящиеДанные</Параметры>
			</Алгоритм>
			<Алгоритм Имя="ПрочитатьОтветственноеЛицо" ИспользуетсяПриЗагрузке="false">
				<Текст>// Параметры:
// - Источник
// - ОтветственноеЛицо

Отбор = Новый Структура("СтруктурнаяЕдиница, ОтветственноеЛицо");
Отбор.СтруктурнаяЕдиница = Источник.Ссылка;
Отбор.ОтветственноеЛицо  = Перечисления.ОтветственныеЛицаОрганизаций[ОтветственноеЛицо];

СрезПоследних = РегистрыСведений.ОтветственныеЛицаОрганизаций.СрезПоследних(ТекущаяДата(), Отбор);

Если СрезПоследних.Количество() &lt; 1 Тогда
	Значение = Неопределено;
Иначе
	Значение  = СрезПоследних[0].ФизическоеЛицо;
	Должность = СрезПоследних[0].Должность;
КонецЕсли;</Текст>
				<Параметры>Источник, ОтветственноеЛицо</Параметры>
			</Алгоритм>
			<Алгоритм Имя="СоздатьИВыгрузитьБанковскийСчетКонтрагента" ИспользуетсяПриЗагрузке="false">
				<Текст>// Параметры:
// - БанковскийСчетКонтрагента;
// - НовыйБанковскийСчет;
// - НовыйКонтрагент

Если ЗначениеЗаполнено(БанковскийСчетКонтрагента) Тогда
	
	Запрос = Новый Запрос("ВЫБРАТЬ ПЕРВЫЕ 1
		|	БанковскиеСчета.Ссылка КАК СсылкаНаСчет
		|ИЗ
		|	Справочник.БанковскиеСчета КАК БанковскиеСчета
		|ГДЕ
		|	БанковскиеСчета.НомерСчета = &amp;НомерСчета
		|	И БанковскиеСчета.Владелец = &amp;Владелец");
		
	Запрос.УстановитьПараметр("НомерСчета", БанковскийСчетКонтрагента.НомерСчета);
	Запрос.УстановитьПараметр("Владелец",   НовыйКонтрагент);
	
	Выборка = Запрос.Выполнить().Выбрать();
	Если Выборка.Следующий() Тогда
		НовыйБанковскийСчет = Выборка.СсылкаНаСчет;
	Иначе
		
		НовыйБанковскийСчет = Справочники.БанковскиеСчета.СоздатьЭлемент();
		ЗаполнитьЗначенияСвойств(НовыйБанковскийСчет, БанковскийСчетКонтрагента,, "Владелец, Родитель, Код");
		
		НовыйБанковскийСчет.Владелец = НовыйКонтрагент;
		НовыйБанковскийСчет.УстановитьНовыйКод();
		НовыйБанковскийСчет.Записать();
		ЗарегистрироватьОбъектСозданныйПриВыгрузке(НовыйБанковскийСчет.Ссылка);
		
		ВыгрузитьПоПравилу(НовыйБанковскийСчет, , , ,"БанковскиеСчетаКонтрагентов");
		НовыйБанковскийСчет = НовыйБанковскийСчет.Ссылка;
	КонецЕсли;
	
КонецЕсли;</Текст>
				<Параметры>БанковскийСчетКонтрагента, НовыйБанковскийСчет, НовыйКонтрагент</Параметры>
			</Алгоритм>
			<Алгоритм Имя="СоздатьИВыгрузитьКонтрагентаИзФизЛица" ИспользуетсяПриЗагрузке="false">
				<Текст>// Параметры:
// - ФизЛицо

Если ЗначениеЗаполнено(ФизЛицо) Тогда
	
	// Проверим, есть ли в базе контрагент, созданный из физ. лица, если нету, то создадим его и выгрузим
	СсылкаНаКонтрагента = Справочники.Контрагенты.ПолучитьСсылку(ФизЛицо.УникальныйИдентификатор());
	
	Если Не ОбщегоНазначения.СсылкаСуществует(СсылкаНаКонтрагента) Тогда
		
		НовыйКонтрагент = Справочники.Контрагенты.СоздатьЭлемент();
		НовыйКонтрагент.Наименование              = ФизЛицо.Наименование;
		НовыйКонтрагент.ИНН                       = ФизЛицо.ИНН;
		НовыйКонтрагент.ЮридическоеФизическоеЛицо = Перечисления.ЮридическоеФизическоеЛицо.ФизическоеЛицо;
		НовыйКонтрагент.НаименованиеПолное        = ФизЛицо.НаименованиеСлужебное;
		НовыйКонтрагент.ГоловнойКонтрагент        = СсылкаНаКонтрагента;
		
		НовыйКонтрагент.УстановитьСсылкуНового(СсылкаНаКонтрагента);
		НовыйКонтрагент.Записать();
		
		ЗарегистрироватьОбъектСозданныйПриВыгрузке(НовыйКонтрагент.Ссылка);
		ВыгрузитьПоПравилу(НовыйКонтрагент,,,,"Контрагенты");
		
	КонецЕсли;
	
	НовыйКонтрагент = СсылкаНаКонтрагента;
	
КонецЕсли;</Текст>
				<Параметры>ФизЛицо, НовыйКонтрагент</Параметры>
			</Алгоритм>
			<Алгоритм Имя="СформироватьСписокСчетовПрочихРасходов" ИспользуетсяПриЗагрузке="false">
				<Текст>СписокСчетовПрочихРасходов.Добавить(ПланыСчетов.Хозрасчетный.ОсновноеПроизводство, "20.01");
СписокСчетовПрочихРасходов.Добавить(ПланыСчетов.Хозрасчетный.ВспомогательныеПроизводства, "23");
СписокСчетовПрочихРасходов.Добавить(ПланыСчетов.Хозрасчетный.ОбщепроизводственныеРасходы, "25");
СписокСчетовПрочихРасходов.Добавить(ПланыСчетов.Хозрасчетный.ОбщехозяйственныеРасходы, "26");
СписокСчетовПрочихРасходов.Добавить(ПланыСчетов.Хозрасчетный.ИздержкиОбращения, "44.01");
СписокСчетовПрочихРасходов.Добавить(ПланыСчетов.Хозрасчетный.КоммерческиеРасходы, "44.02");
СписокСчетовПрочихРасходов.Добавить(ПланыСчетов.Хозрасчетный.ПрочиеРасходы, "91.02");</Текст>
